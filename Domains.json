[
  {
    "Name": "dilbert.com",
    "Url": "http://dilbert.com",
    "Path": "^/strip/((?:[0-9]+-?)+$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".img-comic"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Text",
        "Selector": "title",
        "PatternString": "^.*?-  Dilbert Comic Strip on (\\d{4}-\\d{2}-\\d{2}).*$"
      },
      {
        "Type": "Text",
        "Selector": ".comic-title-name"
      }
    ],
    "FileNameFragmentDelimiter": "_-_"
  },
  {
    "Name": "www.ohjoysextoy.com",
    "Url": "https://www.ohjoysextoy.com",
    "PatternString": "^/([^/]+)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".comicpane img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": ".comicpane > img"
      }
    ]
  },
  {
    "Name": "turnoff.us",
    "Url": "http://turnoff.us",
    "PatternString": "/geek/(.*?)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".post-content img",
        "Replace": "http://turnoff.us$"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".post-title"
      }
    ]
  },
  {
    "Name": "www.schisslaweng.net",
    "Url": "https://www.schisslaweng.net",
    "Path": "^/(.*?)/.*$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".gallery-item img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name=\"shareaholic:article_published_time\"]",
        "PatternString": "^(\\d{4}-\\d{2}-\\d{2}).*$"
      },
      {
        "Type": "Text",
        "Selector": "h1"
      }
    ],
    "FileNameFragmentDelimiter": "_-_"
  }
] 