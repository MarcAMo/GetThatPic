[
  {
    "Name": "dilbert.com",
    "Url": "/http:\/\/dilbert.com/",
    "Path": "/^\/strip\/((?:[0-9]+-?)+)$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".img-comic"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Text",
        "Selector": "title",
        "Pattern": "/^(?:.*?-)? *Dilbert Comic Strip on ([0-9]{4}-[0-9]{2}-[0-9]{2}).*$/"
      },
      {
        "Type": "Text",
        "Selector": ".comic-title-name"
      }
    ]
  },
  {
    "Name": "ohjoysextoy.com",
    "Url": "/https:\/\/www.ohjoysextoy.com/",
    "Path": "/^\/([^\/]+)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".comicpane img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".comicpane > img",
        "Pattern": "/^.*\/([0-9]{4}-[0-9]{2}-[0-9]{2}).*/"
      },
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": ".comicpane > img"
      }
    ]
  },
  {
    "Name": "turnoff.us",
    "Url": "/http:\/\/turnoff.us/",
    "Path": "/^\/geek\/(.*?)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".post-content img",
        "Replace": "http://turnoff.us$1"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".post-title",
        "Pattern": "/^(?:\\$ )(.*)$?/"
      }
    ]
  },
  {
    "Name": "thecodinglove.com",
    "Url": "/http:\/\/thecodinglove.com/",
    "Path": "/^\/post\/([0-9]+)((?:\/[^\/]*\/?)?)$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".post img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".post h3"
      }
    ]
  },
  {
    "Name": "xkcd.com",
    "Url": "/https?:\/\/(?:www\\.)?xkcd.com/",
    "Path": "/^\/([0-9]+)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic > a > img",
        "Pattern": "/^(.*)(\\.[a-zA-Z0-9]+)$/",
        "Replace": "$1_large$2"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "Pattern": "/^\/([0-9]+)\/$/"
      },
      {
        "Type": "Html",
        "Selector": "#ctitle"
      },
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": "#comic img",
        "Replace": "$1"
      }
    ]
  },
  {
    "Name": "giphy.com",
    "Url": "/https:\/\/giphy.com/",
    "Path": "/^\/?gifs/([a-zA-Z0-9-]+)/?(?:\\?.*)?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='og:image']",
        "Pattern": "/^.*?\/media\/([^\/]+)\/.*$/",
        "Replace": "https://i.giphy.com/media/$1/giphy.gif"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='keywords']",
        "Pattern": "/(?:(?:\\s|^)GIF|(?:\\s|^)Animated|(?:\\s|^)hd(?:,|$)|,)/ig",
        "Replace": ""
      }
    ]
  },
  {
    "Name": "media.giphy.com",
    "Url": "/https?:\/\/media[0-9]*\\.giphy\\.com/",
    "Path": "/^\/?media/([a-zA-Z0-9-]+)\/giphy\\.gif(?:\\?.*)?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='og:image']",
        "Pattern": "/^.*?\/media\/([^\/]+)\/.*$/",
        "Replace": "https://i.giphy.com/media/$1/giphy.gif"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='keywords']",
        "Pattern": "/(?:(?:\\s|^)GIF|(?:\\s|^)Animated|(?:\\s|^)hd(?:,|$)|,)/ig",
        "Replace": ""
      }
    ]
  },
  {
    "Name": "Schisslaweng",
    "Url": "/https:\/\/(?:www\\.)?schisslaweng\\.net/",
    "Path": "/^\/(.*?)\/.*$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".gallery-item img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name=\"shareaholic:article_published_time\"]",
        "Pattern": "/^([0-9]{4}-[0-9]{2}-[0-9]{2}).*$/"
      },
      {
        "Type": "Text",
        "Selector": "h1"
      }
    ]
  },
  {
    "Name": "The Gamer Cat",
    "Url": "/http:\/\/(?:www\\.)?thegamercat\\.com/",
    "Path": "/^\/comic\/([0-9a-zA-Z-]+)\/$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img",
        "Pattern": "/^((?:\\/?[^\\/]+\\/)+(?:[^_]*_)+)(\\d+)(\\.[a-zA-z0-9]{2,4})$/",
        "Replace": "$2"
      },
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='article:published_time']",
        "Pattern": "/^([0-9]{4}-[0-9]{2}-[0-9]{2}).*$/"
      },
      {
        "Type": "Text",
        "Selector": "#content > article > .post-content > .post-info > h2"
      },
      {
        "Type": "Text",
        "Selector": ".entry > p:first-child"
      }
    ]
  },
  {
    "Name": "awkwardzombie.com",
    "Url": "/https?:\/\/(?:www\\.)?awkwardzombie\\.com/",
    "Path": "/^\/?(?:index\\.php)?\\?(?:page=[0-9]+&)?comic=([0-9]{2})([0-9]{2})([0-9]{2})$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "Pattern": "/^\/?(?:index\\.php)?\\?(?:page=[0-9]+&)?comic=([0-9]{2})([0-9]{2})([0-9]{2})\/?$/",
        "Replace": "20$3-$1-$2"
      },
      {
        "Type": "Text",
        "Selector": "#blarg > div:nth-child(3)",
        "Pattern": "/^\\s*(.*?)\\s*$/"
      },
      {
        "Type": "Text",
        "Selector": "#site_content > .title.centered",
        "Pattern": "/^\\s*(.*?)\\s*$/"
      }
    ]
  },
  {
    "Name": "Das Leben ist kein Ponyhof",
    "Url": "/http:\/\/(?:www\\.)?sarahburrini\\.com/",
    "Path": "/^\/wordpress\/comic\/(.*?)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img",
        "Pattern": "/^https?:\/\/(?:www\\.)?sarahburrini\\.com\/wordpress\/wp-content\/uploads\/[0-9]+\/[0-9]+\/([0-9]{4}-[0-9]{2}-[0-9]{2}).*$/"
      },
      {
        "Type": "Attribute",
        "AttributeName": "alt",
        "Selector": "#comic img"
      }
    ]
  },
  {
    "Name": "phdcomics.com",
    "Url": "/https?:\/\/(?:www\\.)?phdcomics\\.com/",
    "Path": "/^\/?comics(?:\/archive)?\\.php\\?(?:comicid|f)=([0-9]+)$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='twitter:image']"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name=\"twitter:image\"]",
        "Pattern": "/^(?:.*\/)*phd([0-9]{2})([0-9]{2})([0-9]{0,2})s?(?:\\..*)$/",
        "Replace": "20$3-$1-$2"
      },
      {
        "Type": "Text",
        "Selector": "title",
        "Pattern": "/^(?:(?:\\t|\\r|\\n|.)*?: )(.*)$/"
      }
    ]
  },
  {
    "Name": "Cyanide & Happiness",
    "Url": "/http:\/\/explosm.net/",
    "Path": "/^\/comics\/(\\d+)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#main-comic",
        "Pattern": "/^([^\\?]+).*$/"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "Pattern": "/^/comics/([0-9]+)/?/"
      },
      {
        "Type": "Text",
        "Selector": ".meta-data h3.past-week-comic-title a",
        "Pattern": "/^([0-9]{4})\\.([0-9]{2})\\.([0-9]{2})$/",
        "Replace": "$1-$2-$3"
      },
      {
        "Type": "Text",
        "Selector": ".author-credit-name",
        "Pattern": "/^by (.*?)$/"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#main-comic",
        "Pattern": "/.*/([^\\?$]+)\\..*/"
      }
    ]
  },
  {
    "Name": "nichtlustig.de",
    "Url": "/http:\/\/(?:static\\.)?nichtlustig\\.de/",
    "Path": "/^/toondb/([0-9]+)\\.html$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "link[rel=\"image_src\"]"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "Pattern": "/^/toondb/([0-9]{2})([0-9]{2})([0-9]{2})\\.html$/",
        "Replace": "20$1-$2-$3"
      },
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name=\"title\"]",
        "Pattern": "/^NICHTLUSTIG Cartoon - (.+)$/"
      }
    ]
  },
  {
    "Name": "ruthe.de",
    "Url": "/http://(?:www\\.)?ruthe\\.de/",
    "Path": "//?cartoon/([0-9]+)(?:/datum(?:/(?:asc|desc))?)?/?/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "link[rel=\"image_src\"]"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "link[rel=\"image_src\"]",
        "Pattern":  "/^.*\/strip_([0-9]+)\\..*$/" 
      }
    ]
  },
  {
    "Name": "totaberlustig.com",
    "Url": "/https?:\/\/(?:www\\.)?totaberlustig\\.com/",
    "Path": "/^\/?([^\\\/]+)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Text",
        "Selector": ".post-text small",
        "Pattern": "/^.*? am ([0-9]+)/([0-9]+)/([0-9]+).*$/",
        "Replace": "20$3-$2-$1"
      },
      {
        "Type": "Text",
        "Selector": ".post-text a[rel=\"author\"]"
      },
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": "#comic img"
      }
    ]
  },
  {
    "Name": "Marvin Cliffords Mittenmang",
    "Url": "/https?:\/\/www\\.marvcomics\\.com/",
    "Path": "/^\/?comics\/mittenmang\/([^\\\/]*)\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".singlecomic img",
        "Pattern": "/^([^?]+).*$/"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Text",
        "Selector": "title", 
        "Pattern": "/^[^#]+#([0-9]+).*$/"
      },
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='shareaholic:article_published_time']",
        "Pattern": "/^([0-9]{4}-[0-9]{2}-[0-9]{2}).*$/"
      },
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": ".singlecomic > a",
        "Pattern": "/^[^_]+_(.+?)_(?:FINAL|web).*$/"
      }
    ]
  },
  {
    "Name": "questionablecontent.net",
    "Url": "/http:\/\/(?:www\\.)?questionablecontent\\.net/",
    "Path": "/^\/view.php\\?comic=([0-9]+)$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#strip",
        "Pattern": "/^\\./",
        "Replace": "http://www.questionablecontent.net"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#strip",
        "Pattern": "/^(?:[^\/]*\/)*([0-9]+)(\\.[a-z]+)$/"
      }
    ]
  },
  {
    "Name": "userfriendly.org",
    "Url": "/http:\/\/(?:ars\\.|www\\.)?userfriendly\\.org/",
    "Path": "/^\/cartoons\/?\\?id=([0-9]+)$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "a[href^=\"/cartoons/?id=\"] > img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "a[href^=\"/cartoons/?id=\"]",
        "Pattern": "/^\\/cartoons\\/\\?id=([0-9]{4,4})([0-9]{2,2})([0-9]{2,2})$/",
        "Replace": "$1-$2-$3"
      }
    ]
  },
  {
    "Name": "graphitti-blog.de",
    "Url": "/https?:\/\/(?:www\\.)?graphitti-blog\\.de/",
    "Path": "/^\/[0-9]{4}\/[0-9]{2}\/[0-9]{2}\/[0-9a-zA-Z-]+\/?$/",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": ".entry-content > p > a"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".entry-content > p > img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "Pattern": "/^.*?\/([0-9]{4})\/([0-9]{2})\/([0-9]{2})\/.*$/",
        "Replace":  "$1-$2-$3" 
      },
      {
        "Type": "Html",
        "Selector": ".entry-title"
      }
    ]
  }
] 