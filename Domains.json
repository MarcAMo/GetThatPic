[
  {
    "Name": "dilbert.com",
    "Url": "http://dilbert.com",
    "Path": "^/strip/((?:[0-9]+-?)+$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".img-comic"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Text",
        "Selector": "title",
        "PatternString": "^.*?-  Dilbert Comic Strip on (\\d{4}-\\d{2}-\\d{2}).*$"
      },
      {
        "Type": "Text",
        "Selector": ".comic-title-name"
      }
    ]
  },
  {
    "Name": "www.ohjoysextoy.com",
    "Url": "https://www.ohjoysextoy.com",
    "PatternString": "^/([^/]+)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".comicpane img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": ".comicpane > img"
      }
    ]
  },
  {
    "Name": "turnoff.us",
    "Url": "http://turnoff.us",
    "PatternString": "/geek/(.*?)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".post-content img",
        "Replace": "http://turnoff.us$"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".post-title"
      }
    ]
  },
  {
    "Name": "thecodinglove.com",
    "Url": "http://thecodinglove.com",
    "Path": "^/post/([0-9]+)(/[^/]+)?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#post1 img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": "#post1 h3"
      }
    ]
  },
  {
    "Name": "xkcd.com",
    "Url": "http://xkcd.com",
    "Path": "^/([0-9]+)/$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "#comic>a"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "PatternString": "^/([0-9]+)/$"
      },
      {
        "Type": "Html",
        "Selector": "#ctitle"
      },
      {
        "Type": "Attribute",,
        "AttributeName": "title",
        "Selector": "#comic img",
        "Replace": "- $1"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "www.schisslaweng.net",
    "Url": "https://www.schisslaweng.net",
    "Path": "^/(.*?)/.*$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".gallery-item img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name=\"shareaholic:article_published_time\"]",
        "PatternString": "^(\\d{4}-\\d{2}-\\d{2}).*$"
      },
      {
        "Type": "Text",
        "Selector": "h1"
      }
    ]
  },
  {
    "Name": "giphy.com",
    "Url": "https://giphy.com",
    "Path": "^/gifs/([a-zA-Z0-9-]+)/?(?:\\?.*)?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='og:image']",
        "PatternString": "^.*?\/media\/([^\/]+)\/.*$",
        "Replace": "https://i.giphy.com/media/$1/giphy.gif"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='keywords']",
        "PatternString": "( GIF| Animated|,)",
        "Replace": ""
      }
    ]
  },
  {
    "Name": "media.giphy.com",
    "Url": "https://media.giphy.com",
    "Path": "^/media/([a-zA-Z0-9-]+)/giphy\\.gif(?:\\?.*)?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='og:image']",
        "PatternString": "^.*?\/media\/([^\/]+)\/.*$",
        "Replace": "https://i.giphy.com/media/$1/giphy.gif"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='keywords']",
        "PatternString": "( GIF| Animated|,)",
        "Replace": ""
      }
    ]
  },
  {
    "Name": "www.thegamercat.com",
    "Url": "http://www.thegamercat.com",
    "Path": "^/comic/([0-9a-zA-Z-]+)/$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img",
        "PatternString": "^((?:\\/?[^\\/]+\\/)+(?:[^_]*_)+)(\\d+)(\\.[a-zA-z0-9]{2,4})$",
        "Replace": "$2"
      },
      {
        "Type": "Html",
        "Selector": "#content > article > .post-content > .post-info > h2"
      }
    ],
    "FileNameFragmentDelimiter": " "
  }
] 