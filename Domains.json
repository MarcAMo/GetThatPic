[
  {
    "Name": "dilbert.com",
    "Url": "http://dilbert.com",
    "Path": "^/strip/((?:[0-9]+-?)+$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".img-comic"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Text",
        "Selector": "title",
        "PatternString": "^.*?-  Dilbert Comic Strip on (\\d{4}-\\d{2}-\\d{2}).*$"
      },
      {
        "Type": "Text",
        "Selector": ".comic-title-name"
      }
    ]
  },
  {
    "Name": "www.ohjoysextoy.com",
    "Url": "https://www.ohjoysextoy.com",
    "PatternString": "^/([^/]+)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".comicpane img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": ".comicpane > img"
      }
    ]
  },
  {
    "Name": "turnoff.us",
    "Url": "http://turnoff.us",
    "PatternString": "/geek/(.*?)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".post-content img",
        "Replace": "http://turnoff.us$"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".post-title"
      }
    ]
  },
  {
    "Name": "thecodinglove.com",
    "Url": "http://thecodinglove.com",
    "Path": "^/post/([0-9]+)(/[^/]+)?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#post1 img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": "#post1 h3"
      }
    ]
  },
  {
    "Name": "xkcd.com",
    "Url": "http://xkcd.com",
    "Path": "^/([0-9]+)/$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "#comic>a"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "PatternString": "^/([0-9]+)/$"
      },
      {
        "Type": "Html",
        "Selector": "#ctitle"
      },
      {
        "Type": "Attribute",,
        "AttributeName": "title",
        "Selector": "#comic img",
        "Replace": "- $1"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "www.schisslaweng.net",
    "Url": "https://www.schisslaweng.net",
    "Path": "^/(.*?)/.*$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".gallery-item img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name=\"shareaholic:article_published_time\"]",
        "PatternString": "^(\\d{4}-\\d{2}-\\d{2}).*$"
      },
      {
        "Type": "Text",
        "Selector": "h1"
      }
    ]
  },
  {
    "Name": "giphy.com",
    "Url": "https://giphy.com",
    "Path": "^/gifs/([a-zA-Z0-9-]+)/?(?:\\?.*)?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='og:image']",
        "PatternString": "^.*?\/media\/([^\/]+)\/.*$",
        "Replace": "https://i.giphy.com/media/$1/giphy.gif"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='keywords']",
        "PatternString": "( GIF| Animated|,)",
        "Replace": ""
      }
    ]
  },
  {
    "Name": "media.giphy.com",
    "Url": "https://media.giphy.com",
    "Path": "^/media/([a-zA-Z0-9-]+)/giphy\\.gif(?:\\?.*)?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[property='og:image']",
        "PatternString": "^.*?\/media\/([^\/]+)\/.*$",
        "Replace": "https://i.giphy.com/media/$1/giphy.gif"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name='keywords']",
        "PatternString": "( GIF| Animated|,)",
        "Replace": ""
      }
    ]
  },
  {
    "Name": "www.thegamercat.com",
    "Url": "http://www.thegamercat.com",
    "Path": "^/comic/([0-9a-zA-Z-]+)/$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img",
        "PatternString": "^((?:\\/?[^\\/]+\\/)+(?:[^_]*_)+)(\\d+)(\\.[a-zA-z0-9]{2,4})$",
        "Replace": "$2"
      },
      {
        "Type": "Html",
        "Selector": "#content > article > .post-content > .post-info > h2"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "www.awkwardzombie.com",
    "Url": "http://www.awkwardzombie.com,
    "Path": "^/(?:index\\.php)?\\?(?:page=\\d+&)?comic=((\\d{2})(\\d{2})(\\d{2}))/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "PatternString": "^/(?:index\\.php)?\\?(?:page=\\d+&)?comic=((\\d{2})(\\d{2})(\\d{2}))/?$",
        "Replace": "20$3-$1-$2"
      },
      {
        "Type": "Html",
        "Selector": "#blarg > div:last-of-type"
        "PatternString": "^\s*(.*?)\s*$"
      },
      {
        "Type": "Html",
        "Selector": "#site_content > .title.centered"
        "PatternString": "^\s*(.*?)\s*$",
        "Replace": "- $1"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "sarahburrini.com",
    "Url": "http://sarahburrini.com",
    "Path": "^\\/wordpress\\/comic\\/(.*?)\\/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img",
        "PatternString": "^http:\/\/sarahburrini\.com\/wordpress\/wp-content\/uploads\/\d+\/\d+\/(\d{4}-\d{2}-\d{2}).*$"
      },
      {
        "Attribute": "Html",
        "Selector": ".post-title"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "www.phdcomics.com",
    "Url": "http://www.phdcomics.com",
    "Path": "^\/comics.php\?f=(\d+)$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic",
        "PatternString": "^(?:[^/]+\/+)+(phd)(\d{2})(\d{2})(\d{2})s(\.gif)$",
        "Replace": "$1$4$2$3"
      }
    ]
  },
  {
    "Name": "Cyanide & Happiness",
    "Url": "http://explosm.net",
    "Path": "^/comics/(\d+)/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#main-comic",
        "PatternString": "([^?]+).*"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Url",
        "PatternString": "^/comics/([0-9]+)/?"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#main-comic",
        "PatternString": ".*\/([^\?$]+)\..*"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "nichtlustig.de",
    "Url": "http://static.nichtlustig.de",
    "Path": "^/toondb/([0-9]+)\.html$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "link[rel="image_src"]"
      },
    "FileNameFragments": [
      {
        "Type": "Url",
        "PatternString": "^/toondb/([0-9]+)\.html$"
      },
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "Selector": "meta[name="title"]",
        "PatternString": "^NICHTLUSTIG Cartoon - (.+)$"
      }
    ],
    "FileNameFragmentDelimiter": " "
    ]
  },
  {
    "Name": "ruthe.de",
    "Url": "http://ruthe.de",
    "Path": "/cartoon/$1",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "PatternString": "^/",
        "Replace": "http://ruthe.de/"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "content",
        "PatternString": "^.*?/cartoons/strip_(\d+)\.[a-z]+$"
      }
    ]
  },
  {
    "Name": "totaberlustig.com",
    "Url": "http://www.totaberlustig.com",
    "Path": "^\/([^\/]*)\/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".post-text small",
        "PatternString": "^.*?<\\/a> am ([0-9]+)\\/([0-9]+)\\/([0-9]+).*$",
        "Replace": "20$3-$2-$1"
      },
      {
        "Type": "Attribute",
        "AttributeName": "title",
        "Selector": "#comic img"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "www.marvcomics.com",
    "Url": "http://www.marvcomics.com",
    "Path": "^\/comics\/(?:mittenmang\/)?([^\/]*)\/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": ".singlecomic img",
        "PatternString": "^([^?]+).*$"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "alt",
        "PatternString": "^[^#]+#([0-9]+).*$"
      },
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "PatternString": "^[^_]+_(.+?)_FINAL.*$"
      }
    ],
    "FileNameFragmentDelimiter": " "
  },
  {
    "Name": "www.questionablecontent.net",
    "Url": "http://www.questionablecontent.net",
    "Path": "^\/view.php\?comic=([0-9]+)$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#strip",
        "PatternString": "^\.",
        "Replace": "http://www.questionablecontent.net"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#strip",
        "PatternString": "^(?:[^\/]*\/)*([0-9]+)(\.[a-z]+)$"
      }
    ]
  },
  {
    "Name": "userfriendly.org",
    "Url": "http://ars.userfriendly.org",
    "Path": "^\/cartoons\/?\?id=([0-9]+)$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "a[href^=\"/cartoons/?id=\"] > img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "a[href^=\"/cartoons/?id=\"]",
        "PatternString": "^\\/cartoons\\/\\?id=([0-9]{4,4})([0-9]{2,2})([0-9]{2,2})$",
        "Replace": "$1-$2-$3"
      }
    ]
  },
  {
    "Name": "www.graphitti-blog.de",
    "Url": "http://www.graphitti-blog.de",
    "Path": "^\\/[0-9]{4}\\/[0-9]{2}\\/[0-9]{2}\\/[0-9a-zA-Z-]+\\/?$",
    "Images": [
      {
        "Type": "Attribute",
        "AttributeName": "href",
        "Selector": "#dslc-theme-content-inner a[href$=\".jpg\"]"
      },
      {
        "Type": "Attribute",
        "AttributeName": "src",
        "Selector": "#dslc-theme-content-inner img"
      }
    ],
    "FileNameFragments": [
      {
        "Type": "Html",
        "Selector": ".entry-title"
      }
    ]
  }
] 